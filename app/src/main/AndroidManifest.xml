<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.kuleuven.fitnessapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessApp">
        <activity
            android:name=".classes.ActivityC.SignUpScreen"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Stretches"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Endurance"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Calisthetics"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Heavy_Lifting"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Cardio"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Categories"
            android:exported="false" />
        <activity
            android:name=".classes.ActivityC.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>